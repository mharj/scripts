[Unit]
Description=etcd key-value store
Documentation=https://github.com/coreos/etcd
After=docker.service

[Service]
ExecStartPre=/bin/sh -c 'docker stop etcd 2>/dev/null'
ExecStartPre=/bin/sh -c 'docker rm etcd 2>/dev/null'
ExecStart=/usr/bin/docker run --net=host --name etcd quay.io/coreos/etcd:v2.3.6
#ExecStop=/usr/bin/docker stop etcd
Restart=always
RestartSec=10s

[Install]
WantedBy=multi-user.target
