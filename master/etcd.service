[Unit]
Description=etcd key-value store
Documentation=https://github.com/coreos/etcd
After=docker.service

[Service]
ExecStartPre=docker stop etcd 2>/dev/null
ExecStartPre=docker rm etcd 2>/dev/null
ExecStart=docker run --net=host --name etcd quay.io/coreos/etcd:v2.3.6
#ExecStop=docker stop etcd
Restart=always
RestartSec=10s

[Install]
WantedBy=multi-user.target
